def add_underscore(txt):
    vowels='aeiouAEIOU'
    result=[]
    count =0

    for i in range(len(txt)):
        result.append(txt[i])
        count+=1

        if count==3:
            if txt[i] not in vowels:
                if i+1<len(txt):
                    result.append("_")
            count=0
    return "".join(result)

