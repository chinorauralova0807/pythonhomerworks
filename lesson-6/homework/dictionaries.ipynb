def uncommon_elements(list1,list2):
    uncommon=[]
    for num in list1:
        if num not in list2:
            uncommon.append(num)
    for num in list2:
        if num not in list1:
            uncommon.append(num)
    return uncommon
print(uncommon_elements([1,1,2],[2,3,4]))

print(uncommon_elements([1,2,3],[4,5,6]))

print(uncommon_elements([1,1,2,3,4,2],[1,3,4,5]))
    
